<?xml version="1.0" encoding="utf-8" ?>
<widget id="AudioVideoPlayback.widget.AudioVideoPlayback" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>Audio Video Playback</name>
    <description>Audio Video Playback</description>
    <icon></icon>
    <properties>
        <property key="showUserInterface" type="boolean" defaultValue="true">
            <caption>Show User Interface</caption>
            <category>Behavior</category>
            <description></description>
        </property>
        <property key="autoPlay" type="boolean" defaultValue="false">
            <caption>Auto Play</caption>
            <category>Behavior</category>
            <description>Automatic Start play file when loaded</description>
        </property>
        <property key="fullScreen" type="boolean" defaultValue="false">
            <caption>Start Full Screen</caption>
            <category>Behavior</category>
            <description>Start in full screen (video only)</description>
        </property>
        <property key="loop" type="boolean" defaultValue="false">
            <caption>Loop</caption>
            <category>Behavior</category>
            <description>restart when finished playing file</description>
        </property>
        <property key="videoWidth" type="integer" defaultValue="640">
            <caption>Width</caption>
            <category>Video</category>
            <description>Width of the video in pixels</description>
        </property>
        <property key="videoHeight" type="integer" defaultValue="360">
            <caption>Height</caption>
            <category>Video</category>
            <description>Height of the video in pixels</description>
        </property>
        <property key="videoCssClass" type="string" defaultValue="jp-video-360p">
            <caption>Css Class</caption>
            <category>Video</category>
            <description>Theme CSS class</description>
        </property>
        <property key="showPlayControl" type="boolean" defaultValue="true">
            <caption>Play control</caption>
            <category>Display</category>
            <description></description>
        </property>
        <property key="showAudioControl" type="boolean" defaultValue="true">
            <caption>Audio control</caption>
            <category>Display</category>
            <description></description>
        </property>
        <property key="showProgressBar" type="boolean" defaultValue="true">
            <caption>Progress bar</caption>
            <category>Display</category>
            <description></description>
        </property>
        <property key="showTitle" type="boolean" defaultValue="true">
            <caption>Title</caption>
            <category>Display</category>
            <description></description>
        </property>
        <property key="showTime" type="boolean" defaultValue="true">
            <caption>Time</caption>
            <category>Display</category>
            <description></description>
        </property>
        <property key="showToggles" type="boolean" defaultValue="true">
            <caption>Toggles</caption>
            <category>Display</category>
            <description></description>
        </property>
        <property key="showFullScreenToggle" type="boolean" defaultValue="true">
            <caption>Enable Full screen</caption>
            <category>Display</category>
            <description></description>
        </property>
    </properties>
</widget>
